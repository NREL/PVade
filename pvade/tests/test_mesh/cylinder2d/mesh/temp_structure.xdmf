<?xml version="1.0"?>
<!DOCTYPE Xdmf SYSTEM "Xdmf.dtd" []>
<Xdmf Version="3.0" xmlns:xi="http://www.w3.org/2001/XInclude">
  <Domain>
    <Grid Name="temp_mesh" GridType="Uniform">
      <Topology TopologyType="Quadrilateral" NumberOfElements="0" NodesPerElement="4">
        <DataItem Dimensions="0 4" NumberType="Int" Format="HDF">temp_structure.h5:/Mesh/temp_mesh/topology</DataItem>
      </Topology>
      <Geometry GeometryType="XYZ">
        <DataItem Dimensions="0 3" Format="HDF">temp_structure.h5:/Mesh/temp_mesh/geometry</DataItem>
      </Geometry>
    </Grid>
    <Grid Name="cell_tags" GridType="Uniform">
      <xi:include xpointer="xpointer(/Xdmf/Domain/Grid/Geometry)" />
      <Topology TopologyType="Quadrilateral" NumberOfElements="0" NodesPerElement="4">
        <DataItem Dimensions="0 4" NumberType="Int" Format="HDF">temp_structure.h5:/MeshTags/cell_tags/topology</DataItem>
      </Topology>
      <Attribute Name="cell_tags" AttributeType="Scalar" Center="Cell">
        <DataItem Dimensions="0 1" Format="HDF">temp_structure.h5:/MeshTags/cell_tags/Values</DataItem>
      </Attribute>
    </Grid>
    <Grid Name="facet_tags" GridType="Uniform">
      <xi:include xpointer="xpointer(/Xdmf/Domain/Grid/Geometry)" />
      <Topology TopologyType="PolyLine" NumberOfElements="0" NodesPerElement="2">
        <DataItem Dimensions="0 2" NumberType="Int" Format="HDF">temp_structure.h5:/MeshTags/facet_tags/topology</DataItem>
      </Topology>
      <Attribute Name="facet_tags" AttributeType="Scalar" Center="Cell">
        <DataItem Dimensions="0 1" Format="HDF">temp_structure.h5:/MeshTags/facet_tags/Values</DataItem>
      </Attribute>
    </Grid>
  </Domain>
</Xdmf>
